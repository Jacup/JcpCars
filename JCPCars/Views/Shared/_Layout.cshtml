<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />
</head>
<body>
    <header>
        <div style="overflow: hidden;">
            <a id="contact-header" href="">
                Zadzwoń: +48 123456789
            </a>

            <a id="cart-header-link" href="">
                Rezerwacja: (0)
                <span></span>
            </a>
        </div>
        <a id="main-header-logo" href="">
            <img src="../Content/Images/jcpcars-logo-main.png" class="center" />
        </a>
    </header>

    <div>
        @RenderBody()
    </div>


    <footer>
        <nav>
            <ul>
                <li><a href="">O nas</a></li>
                <li><a href="">Kontakt</a></li>
                <li><a href="">Regulamin</a></li>
            </ul>
        </nav>
        <div id="footer-cd-logo">
            <img src="@Url.Content("/content/images/footer-cd.png")" />
        </div>

        <div id="footer-copyright">
            Copyright &copy; 2019 Jakub Gramburg
        </div>
    </footer>

    @RenderSection("ScriptsBottom", required: false)
</body>
</html>
